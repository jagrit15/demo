pipeline{
    any agent
    
    stages{
        stage("A"){
            steps{
               // Read tabular data from a file
                    def filePath = 'C:\Users\jpand\Downloads\SampleCSVFile_2kb.csv'
                    def tabularData = readFile filePath
                    
                    // Display the tabular data
                    echo "Tabular Data:\n${tabularData}"
            }
            post{
                always{
                    echo "========always========"
                }
                success{
                    echo "========A executed successfully========"
                }
                failure{
                    echo "========A execution failed========"
                }
            }
        }
    }
    post{
        always{
            echo "========always========"
        }
        success{
            echo "========pipeline executed successfully ========"
        }
        failure{
            echo "========pipeline execution failed========"
        }
    }
}